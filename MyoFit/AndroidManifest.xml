<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bran.android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyoFitActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WorkoutSelectActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ExerciseActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".WorkoutCompleteActivity"
            android:label="@string/app_name" > 
        </activity>
        <activity
            android:name=".WorkoutListActivity"
            android:label="@string/app_name" > 
        </activity>
        <activity
           	android:name=".ExerciseListActivity"
           	android:label="@string/app_name">
        </activity>
        <activity
           	android:name=".ExerciseDataActivity"
           	android:label="@string/app_name">
        </activity>
    </application>

</manifest>
